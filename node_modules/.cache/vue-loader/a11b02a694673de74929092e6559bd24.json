,\n\n  methods: {\n    startSlide: function() {\n      this.timer = setInterval(this.next, 4000);\n    },\n\n    next: function() {\n      this.currentIndex += 1;\n    },\n    prev: function() {\n      this.currentIndex -= 1;\n    }\n  },\n\n  computed: {\n    currentImg: function() {\n      return this.images[Math.abs(this.currentIndex) % this.images.length];\n    }\n  }\n};\n</script>"]}]}